{
  "name": "parasoft-findings-vsts",
  "version": "1.0.2",
  "description": "Parasoft Findings enables you to visualize test and static analysis results generated by Parasoft products in Azure DevOps",
  "main": "index.js",
  "scripts": {
    "downloadReportGenerator": "node ./scripts/downloadReportGenerator.js",
    "test": "npm run compile && jasmine",
    "compile": "npx tsc && cd ./PublishParasoftTestResults && npm run compilexsl && cd ..",
    "create-extension": "npm test && npm run downloadReportGenerator && npx tfx-cli extension create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parasoft/parasoft-findings-vsts.git"
  },
  "keywords": [
    "azure",
    "vsts",
    "Dashboards & Reporting",
    "Test Management",
    "Test Automation",
    "Static Analysis",
    "Parasoft",
    "SOAtest",
    "XTest",
    "Build task",
    "Test task"
  ],
  "author": "Parasoft Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/parasoft/parasoft-findings-vsts/issues"
  },
  "homepage": "https://github.com/parasoft/parasoft-findings-vsts#readme",
  "icon": "images/icon.png",
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/jasmine": "^4.3.1",
    "adm-zip": "^0.5.10",
    "jasmine": "^5.0.0"
  },
  "devDependencies": {
    "jasmine-reporters": "^2.5.2"
  }
}
