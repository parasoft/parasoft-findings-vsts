{
    "manifestVersion": 1,
    "id": "parasoft-findings-vsts",
    "name": "Parasoft Findings",
    "version": "1.0.7",
    "publisher": "lukasz-biel",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes" : [ "vso.test_write" ],
    "demands": [],
    "files": [
        {
            "path": "PublishParasoftTestResults"
        },
        {
            "path": "images", "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "publish-test-results-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "PublishParasoftTestResults"
            }
        }
    ],
    "description": "Parasoft Findings enables you to visualize test and static analysis results generated by Parasoft products in Azure DevOps.",
    "icons": {
        "default": "images/icon.png"
    },
    "categories": [
        "Azure Test Plans"
    ],
    "tags": [
        "Dashboards & Reporting",
        "Test Management",
        "Test Automation",
        "Static Analysis",
        "Parasoft",
        "SOAtest",
        "XTest",
        "Build task",
        "Test task"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "links": {
        "learn":  {
            "uri": "https://www1.parasoft.com/doc/link?product=findingsVSTS010"
        },
        "license": {
            "uri": "http://www.apache.org/licenses/LICENSE-2.0"
        },
        "support": {
            "uri": "https://www.parasoft.com/support/"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/parasoft/parasoft-findings-vsts.git"
    },
    "branding": {
        "color": "#004877",
        "theme": "dark"
    },
    "public": false
}
