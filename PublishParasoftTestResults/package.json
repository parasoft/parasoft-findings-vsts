{
  "name": "parasoft-findings-vsts-publish",
  "version": "1.0.7",
  "description": "Azure Pipelines Publish Parasoft Test Results Task",
  "main": "publishtestresults.js",
  "scripts": {
    "compilexsl": "(npm run sarifxsl) && (npm run xunitxsl) && (npm run soatestxsl) ",
    "sarifxsl": "node node_modules/xslt3/xslt3.js -t -xsl:./xsl/sarif.xsl -export:./xsl/sarif.sef.json -nogo",
    "xunitxsl": "node node_modules/xslt3/xslt3.js -t -xsl:./xsl/xunit.xsl -export:./xsl/xunit.sef.json -nogo",
    "soatestxsl": "node node_modules/xslt3/xslt3.js -t -xsl:./xsl/soatest-xunit.xsl -export:./xsl/soatest-xunit.sef.json -nogo",
    "licenses": "license-checker-rseidelsohn --excludePackages \"parasoft-findings-vsts-publish\" --limitAttributes \"licenses,licenseFile,publisher,repository\" --files licenses --relativeLicensePath --json --out licenses/license.json",
    "postinstall": "@echo off && echo Package installed successfully. To generate the SEF stylesheets required for SaxonJS to load and perform transformations, run `npm run compilexsl`. To extract NPM package licenses for packaging into the extension, run `npm run licenses`."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parasoft/parasoft-findings-vsts.git"
  },
  "keywords": [
    "azure",
    "vsts",
    "parasoft"
  ],
  "author": "Parasoft Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/parasoft/parasoft-findings-vsts/issues"
  },
  "homepage": "https://github.com/parasoft/parasoft-findings-vsts#readme",
  "dependencies": {
    "azure-pipelines-task-lib": "^4.3.1",
    "dot-properties": "^1.0.1",
    "fs": "0.0.1-security",
    "sax": "^1.2.4",
    "saxon-js": "^2.5.0",
    "xslt3": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.23",
    "@types/q": "^1.5.5",
    "@types/sax": "^1.0.0",
    "license-checker-rseidelsohn": "^4.2.5"
  }
}
